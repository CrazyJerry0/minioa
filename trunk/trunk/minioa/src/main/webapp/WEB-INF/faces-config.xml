<?xml version="1.0"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	<managed-bean>
		<managed-bean-name>HelloWorld</managed-bean-name>
		<managed-bean-class>HelloWorld</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Lang</managed-bean-name>
		<managed-bean-class>org.minioa.core.Lang</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>MySession</managed-bean-name>
		<managed-bean-class>org.minioa.core.MySession</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>pageSize</property-name>
			<value>10</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Application</managed-bean-name>
		<managed-bean-class>org.minioa.core.Application</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Org</managed-bean-name>
		<managed-bean-class>org.minioa.core.Org</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Department</managed-bean-name>
		<managed-bean-class>org.minioa.core.Department</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DepartmentTree</managed-bean-name>
		<managed-bean-class>org.minioa.core.DepartmentTree</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Job</managed-bean-name>
		<managed-bean-class>org.minioa.core.Job</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>JobTree</managed-bean-name>
		<managed-bean-class>org.minioa.core.JobTree</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>User</managed-bean-name>
		<managed-bean-class>org.minioa.core.User</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Role</managed-bean-name>
		<managed-bean-class>org.minioa.core.Role</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>RoleUserRelation</managed-bean-name>
		<managed-bean-class>org.minioa.core.RoleUserRelation</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Op</managed-bean-name>
		<managed-bean-class>org.minioa.core.Op</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>OpRoleRelation</managed-bean-name>
		<managed-bean-class>org.minioa.core.OpRoleRelation</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TopMenu</managed-bean-name>
		<managed-bean-class>org.minioa.core.TopMenu</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TopMenuRoleRelation</managed-bean-name>
		<managed-bean-class>org.minioa.core.TopMenuRoleRelation</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>LeftMenu</managed-bean-name>
		<managed-bean-class>org.minioa.core.LeftMenu</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>LeftMenuRoleRelation</managed-bean-name>
		<managed-bean-class>org.minioa.core.LeftMenuRoleRelation</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Pinyin</managed-bean-name>
		<managed-bean-class>org.minioa.core.Pinyin</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<application>
		<navigation-handler>org.richfaces.ui.application.StateNavigationHandler</navigation-handler>
		<el-resolver>org.richfaces.el.StateELResolver</el-resolver>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
</faces-config>
